#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/krayt78/Desktop/git/personal/test_polka_malt/node_modules/.pnpm/@polkadot-api+cli@0.13.2_jiti@2.4.2_postcss@8.5.4/node_modules/@polkadot-api/cli/dist/node_modules:/home/krayt78/Desktop/git/personal/test_polka_malt/node_modules/.pnpm/@polkadot-api+cli@0.13.2_jiti@2.4.2_postcss@8.5.4/node_modules/@polkadot-api/cli/node_modules:/home/krayt78/Desktop/git/personal/test_polka_malt/node_modules/.pnpm/@polkadot-api+cli@0.13.2_jiti@2.4.2_postcss@8.5.4/node_modules/@polkadot-api/node_modules:/home/krayt78/Desktop/git/personal/test_polka_malt/node_modules/.pnpm/@polkadot-api+cli@0.13.2_jiti@2.4.2_postcss@8.5.4/node_modules:/home/krayt78/Desktop/git/personal/test_polka_malt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/krayt78/Desktop/git/personal/test_polka_malt/node_modules/.pnpm/@polkadot-api+cli@0.13.2_jiti@2.4.2_postcss@8.5.4/node_modules/@polkadot-api/cli/dist/node_modules:/home/krayt78/Desktop/git/personal/test_polka_malt/node_modules/.pnpm/@polkadot-api+cli@0.13.2_jiti@2.4.2_postcss@8.5.4/node_modules/@polkadot-api/cli/node_modules:/home/krayt78/Desktop/git/personal/test_polka_malt/node_modules/.pnpm/@polkadot-api+cli@0.13.2_jiti@2.4.2_postcss@8.5.4/node_modules/@polkadot-api/node_modules:/home/krayt78/Desktop/git/personal/test_polka_malt/node_modules/.pnpm/@polkadot-api+cli@0.13.2_jiti@2.4.2_postcss@8.5.4/node_modules:/home/krayt78/Desktop/git/personal/test_polka_malt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@polkadot-api/cli/dist/main.js" "$@"
else
  exec node  "$basedir/../@polkadot-api/cli/dist/main.js" "$@"
fi
